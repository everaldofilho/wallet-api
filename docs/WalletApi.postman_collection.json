	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"pm.sendRequest({",
					"    url: pm.variables.get('base_url') + '/api/login_check',",
					"    method: 'POST',",
					"    header: 'content-type:multipart/form-data; boundary=postman',",
					"    body: '--postman\\r\\n' + ",
					"        'Content-Disposition: form-data; name=\"username\"\\r\\n' +",
					"        '\\r\\n' +",
					"        pm.variables.get('username') + '\\r\\n' +",
					"        '--postman\\r\\n' +",
					"        'Content-Disposition: form-data; name=\"password\\r\\n' +",
					"        '\\r\\n' +",
					"        pm.variables.get('password') + '\\r\\n' +",
					"        '--postman--'",
					"}, function (error, response) {",
					"    response = response.json();",
					"    if (! error && typeof response.token !== 'undefined') {",
					"        pm.variables.set('token', response.token);",
					"    }",
					"});"
				]
			}
